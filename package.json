{
  "name": "agent-spaces",
  "productName": "AgentSpaces",
  "version": "1.0.0",
  "description": "Claude Cowork replica for local workspace automation",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [
    "electron",
    "claude",
    "agents",
    "automation",
    "workspace"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google/generative-ai": "^0.24.1",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "better-sqlite3": "^11.7.0",
    "clsx": "^2.1.1",
    "croner": "^10.0.1",
    "date-fns": "^4.1.0",
    "diff": "^8.0.3",
    "electron-updater": "^6.7.3",
    "grammy": "^1.39.3",
    "lucide-react": "^0.469.0",
    "node-pty": "^1.1.0",
    "openai": "^6.16.0",
    "playwright": "^1.58.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^16.1.0",
    "simple-git": "^3.30.0",
    "tailwind-merge": "^3.0.0",
    "tldraw": "^4.3.0"
  },
  "build": {
    "appId": "com.agentspaces.app",
    "productName": "AgentSpaces",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ]
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "dmares06",
      "repo": "Agent-Spaces"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmares06/Agent-Spaces.git"
  }
}
